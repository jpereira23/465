<!DOCTYPE html>

<html> 
 <head> 
   <title><%= full_title(yield(:title)) %></title>
   <%= stylesheet_link_tag  'application', media:'all', 'data-turbolink-tag' => true %>
   <%= javascript_include_tag 'application', 'data-turbolink-tag' => true %> 
   <%= csrf_meta_tags %> 
   <%= render 'layouts/shim' %>
 </head> 
 <body> 
   <header class="navabar navbar-fixed-top navbar-inverse"> 
     <%= render 'layouts/header' %>
     <div class="container"> 
       <%= link_to "Good Life", "#", id: "logo" %> 
     <nav> 
       <ul class="nav navbar-nav navbar-right"> 
         <li><%= link_to "Home", "#" %></li> 
         <li><%= link_to "Help", "#" %></li>
         <li><%= link_to "Log in", "#" %></li> 
       </ul> 
     </nav> 
   </div> 
   </headers> 
   <div class="container"> 
     <%= yield %> 
     <%= render 'layouts/footer' %> 
     <= debug(params) if Rails.env.development? %>
   </div>
 </body> 
   
</html> 
